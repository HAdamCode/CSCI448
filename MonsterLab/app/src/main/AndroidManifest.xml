<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MonsterLab"
        tools:targetApi="31">
        <activity
            android:name=".MonsterCard.Card"
            android:exported="false"
            android:label="@string/title_activity_monster_card._card"
            android:theme="@style/Theme.MonsterLab" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/title_activity_main"
            android:theme="@style/Theme.MonsterLab">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".painter"
            android:exported="false"
            android:label="@string/title_activity_painter"
            android:theme="@style/Theme.MonsterLab" />
        <activity
            android:name=".Image"
            android:exported="false"
            android:label="@string/title_activity_image"
            android:theme="@style/Theme.MonsterLab" />
        <activity
            android:name=".Card"
            android:exported="false"
            android:label="@string/title_activity_card"
            android:theme="@style/Theme.MonsterLab" />
    </application>

</manifest>